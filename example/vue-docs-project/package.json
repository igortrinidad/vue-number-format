{
  "name": "vue-docs-project",
  "version": "0.0.0",
  "scripts": {
    "serve": "vite",
    "build": "run-p type-check build-only && cp ./README.md ../../docs/README.md",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "tailwind": "npx tailwindcss -i ./src/assets/styles.css -o ./public/output.css --watch",
    "build:tailwind": "npx tailwindcss -i ./src/assets/styles.css -o ./public/output.css --minify"
  },
  "dependencies": {
    "clipboard": "^2.0.11",
    "prismjs": "^1.29.0",
    "vue": "^3.2.38"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^16.11.56",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "lv-button": "^0.0.2-a",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.7.4",
    "vite": "^3.0.9",
    "vue-tsc": "^0.40.7"
  }
}
